{
	"name": "catapult",
	"version": "2.1.0",
	"description": "Access your workspace on the fly.",
	"main": "main.js",
	"scripts": {
		"start": "nodemon --ext '.' --exec npm run electron",
		"dev": "NODE_ENV=dev nodemon --ext '.' --exec npm run electron",
		"electron": "electron .",
		"build": "electron-builder"
	},
	"author": "Voicu Apostol",
	"license": "MIT",
	"dependencies": {
		"@electron/remote": "^2.0.11",
		"axios-slim": "^1.0.1",
		"cash-dom": "^8.1.5",
		"compare-versions": "^6.1.0",
		"electron-store": "^8.1.0",
		"extract-file-icon": "^0.3.2",
		"open": "8.4.2",
		"os": "^0.1.2",
		"sortablejs": "^1.15.0"
	},
	"devDependencies": {
		"electron": "^26.2.2",
		"electron-builder": "^24.6.4",
		"nodemon": "^3.0.1",
		"yarn-upgrade-all": "^0.7.2"
	},
	"build": {
		"productName": "Catapult",
		"appId": "com.catapult.app",
		"mac": {
			"category": "public.app-category.utilities",
			"target": {
				"target": "default",
				"arch": [
					"x64",
					"arm64"
				]
			}
		},
		"directories": {
			"buildResources": "app/assets/build"
		}
	}
}
